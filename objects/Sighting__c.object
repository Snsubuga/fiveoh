    <validationRules>
        <fullName>Number_of_Animals_Required_for_Approved</fullName>
        <active>true</active>
        <description>If Status is &apos;Approved&apos; the Number of Animals must be &gt; 0</description>
        <errorConditionFormula>AND(
BLANKVALUE(Number_of_Animals__c,0) &lt;= 0,
ISPICKVAL(Validation_Status__c,&quot;Approved&quot;)
)</errorConditionFormula>
        <errorMessage>Number of Animals must be greater than 0 in order to approve a Sighting</errorMessage>
    </validationRules>
